@startuml
rectangle "Add content to repository" {
  (New Content Unit)
  rectangle Repository {
    usecase RV1 as "Repository Version 1
    ---
    contains:
    Content Unit"
    usecase RV2 as "Repository Version 2
    ---
    contains:
    Content Unit
    New Content Unit"
    note "Adding new content to repository" as N
  }

  (New Content Unit) ..|> N
  (RV1) -right-> N
  N -right-> (RV2)
}
@enduml